programa {
	funcao inicio (){
		 
		
		//Exercício 6
		//inicio
			//variaveis* 
			
		inteiro a
     	inteiro b
     	inteiro c
     	inteiro d
     	inteiro ee 
     	inteiro f
     	inteiro x
     	inteiro y
     	inteiro conta1
     	inteiro conta2
     	inteiro conta3
     	inteiro conta4
    
	
			//entradas*
		escreva("Digite o valor de a: ")
