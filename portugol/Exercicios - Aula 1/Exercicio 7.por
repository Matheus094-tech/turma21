     	escreva("Digite o valor de b: ")
     	leia (b)
     	escreva("Digite o valor de c: ")
     	leia (c)
     	escreva("Digite o valor de d: ")
     	leia (d)
     	escreva("Digite o valor de e: ")
     	leia (ee)
     	escreva("Digite o valor de f: ")
     	leia (f)


			//processamento*

		  	conta1 = (c*ee) - (b*f)
		  	conta2 = (a*ee) - (b*d)
		  	conta3 = (a*f) - (c*d)
		  	conta4 = (a*ee) - (b*d)
		  	x = (conta1/conta2)
		  	y = (conta3/conta4)

			//saída

		escreva("O valor de X é igual a " +x+  " e o valor de Y é igual a " +y+ ".") 

 			//fim
		
	}
}

